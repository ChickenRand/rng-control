{
	"rules": {
		"arrow-body-style": [1, "as-needed"],
		"arrow-parens" : [2, "as-needed"],
		"arrow-spacing": 2,
		"callback-return": 2,
		"constructor-super": 1,
		"global-require": 1,
		"handle-callback-err": 2,
		"no-class-assign": 2,
		"no-constant-condition": 2,
		"no-const-assign": 2,
		"no-dupe-class-members": 2,
		"no-mixed-requires": 1,
		"no-new-symbol": 2,
		"no-param-reassign": [2, {
			"props": false
		}],
		"no-path-concat": 2,
		"no-process-exit": 2,
		"no-shadow": 0,
		"no-this-before-super": 2,
		"no-use-before-define": [2, "nofunc"],
		"no-useless-computed-key": 2,
		"no-useless-constructor": 2,
		"no-useless-rename": 2,
		"no-var": 2,
		"object-curly-spacing": [2, "never"],
		"object-shorthand": [1, "always"],
		"one-var":[2, "never"],
		"prefer-const": 2,
		"prefer-rest-params": 2,
		"prefer-spread": 2,
		"prefer-template": 1,
		"rest-spread-spacing": 2,
		"strict": [2,"global"],
		"symbol-description": 2,
		"template-curly-spacing": 2,
		"array-bracket-spacing": [2, "never"],
		"array-callback-return": 2,
		"brace-style": [2, "1tbs"],
		"camelcase": [1, {"properties": "never"}],
		"comma-dangle": 2,
		"comma-spacing": 2,
		"comma-style": [2, "last"],
		"computed-property-spacing": [2, "never"],
		"complexity": [2, 10],
		"consistent-return": 1,
		"curly": [2, "all"],
		"dot-location": [2, "property"],
		"dot-notation": [2, {
			"allowPattern": "^[a-z]+(_[a-z]+)+$"
		}],
		"eol-last": 2,
		"eqeqeq": 2,
		"indent": [2, "tab"],
		"guard-for-in": 1,
		"key-spacing": [2, {
			"beforeColon": false,
			"afterColon": true
		}],
		"keyword-spacing": [2, {
			"before": true,
			"after": true,
			"overrides": {
				"return": {"after": true},
				"throw": {"after": true},
				"case": {"after": true }
			}
		}],
		"linebreak-style": [2, "unix"],
		"max-depth": [2, 4],
		"max-params": [2, 5],
		"new-cap": [2, {
			"newIsCap": true,
			"capIsNew": false
		}],
		"new-parens": 2,
		"no-alert": 2,
		"no-array-constructor": 2,
		"no-caller": 2,
		"no-case-declarations": 2,
		"no-cond-assign": [2, "except-parens"],
		"no-debugger": 2,
		"no-delete-var": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-else-return": 2,
		"no-empty": 2,
		"no-empty-function": 1,
		"no-empty-pattern": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-extra-label": 2,
		"no-extra-semi": 1,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-func-assign": 2,
		"no-implicit-coercion": 1,
		"no-implicit-globals": 2,
		"no-implied-eval": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-magic-numbers": [1, {
			"ignore": [-1, 0, 1, 2],
			"ignoreArrayIndexes": true,
			"enforceConst": true,
			"detectObjects": false
		}],
		"no-mixed-spaces-and-tabs": 2,
		"no-multi-spaces": 2,
		"no-native-reassign": 2,
		"no-new-func": 2,
		"no-new-object": 2,
		"no-new-require": 2,
		"no-new-wrappers": 1,
		"no-octal": 2,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-self-assign": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-shadow-restricted-names": 2,
		"no-spaced-func": 2,
		"no-sparse-arrays": 2,
		"no-throw-literal": 2,
		"no-trailing-spaces": 2,
		"no-undef-init": 2,
		"no-underscore-dangle": 0,
		"no-unreachable": 2,
		"no-unsafe-finally": 2,
		"no-unused-expressions": 2,
		"no-unused-vars": [2, {
			"vars": "all",
			"args": "after-used"
		}],
		"no-useless-concat": 2,
		"no-whitespace-before-property": 2,
		"no-with": 2,
		"operator-assignment": 2,
		"quotes": [2, "single"],
		"radix":2,
		"semi": 2,
		"space-before-blocks": 2,
		"space-before-function-paren": [2, {
			"anonymous": "always",
			"named": "never"
		}],
		"space-in-parens": [2, "never"],
		"space-infix-ops": 2,
		"space-unary-ops": 2,
		"spaced-comment": 1,
		"valid-typeof": 2,
		"wrap-iife": [1, "outside"],
		"yoda": 2
	},
	"parserOptions": {
		"ecmaVersion": 6,
	},
	"env": {
		"es6": true,
		"node": true
	}
}
